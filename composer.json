{
    "type": "project",
    "license": "proprietary",
    "minimum-stability": "stable",
    "prefer-stable": true,
    "require": {
        "php": ">=7.4",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "doctrine/cache": "^1.10",
        "doctrine/doctrine-bundle": "^2.4",
        "doctrine/doctrine-migrations-bundle": "^3.1",
        "doctrine/orm": "^2.9",
        "domnikl/statsd": "^3.0",
        "friendsofsymfony/elastica-bundle": "6.0.x-dev",
        "friendsofsymfony/rest-bundle": "^3.0",
        "jms/serializer-bundle": "^3.9",
        "lexik/jwt-authentication-bundle": "^2.12",
        "nelmio/api-doc-bundle": "^4.5",
        "nyholm/psr7": "^1.4",
        "php-amqplib/rabbitmq-bundle": "^2.8",
        "sensio/framework-extra-bundle": "^6.1",
        "sentry/sentry-symfony": "^4.1",
        "stof/doctrine-extensions-bundle": "^1.6",
        "symfony/asset": "5.3.*",
        "symfony/console": "5.3.*",
        "symfony/dotenv": "5.3.*",
        "symfony/expression-language": "5.3.*",
        "symfony/flex": "^1.3.1",
        "symfony/form": "5.3.*",
        "symfony/framework-bundle": "5.3.*",
        "symfony/http-client": "5.3.*",
        "symfony/lock": "5.3.*",
        "symfony/maker-bundle": "^1.33",
        "symfony/monolog-bundle": "^3.7",
        "symfony/options-resolver": "5.3.*",
        "symfony/psr-http-message-bridge": "^2.1",
        "symfony/runtime": "5.3.*",
        "symfony/security-bundle": "5.3.*",
        "symfony/twig-bundle": "5.3.*",
        "symfony/validator": "5.3.*",
        "symfony/webpack-encore-bundle": "^1.12",
        "symfony/yaml": "5.3.*"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/",
            "StatsdBundle\\": "src/StatsdBundle"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "UnitTests\\": "tests/phpunit",
            "CodeceptionUnitTests\\": "tests/unit",
            "IntegrationTests\\": "tests/functional",
            "AcceptanceTests\\": "tests/acceptance"
        }
    },
    "replace": {
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "5.3.*"
        }
    },
    "require-dev": {
        "cebe/php-openapi": "^1.5",
        "codeception/codeception": "^4.1",
        "codeception/module-asserts": "^1.3",
        "codeception/module-datafactory": "^1.1",
        "codeception/module-doctrine2": "^1.1",
        "codeception/module-phpbrowser": "^1.0",
        "codeception/module-rest": "^1.3",
        "codeception/module-symfony": "^2.0",
        "doctrine/doctrine-fixtures-bundle": "^3.4",
        "mockery/mockery": "^1.4",
        "symfony/phpunit-bridge": "^5.3"
    }
}
